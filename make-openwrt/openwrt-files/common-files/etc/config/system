
config system
	option hostname 'Cendrawasih'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option zonename 'Asia/Jakarta'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option zram_comp_algo 'lzo'
	option description 'Cendrawasih Networks'
	option notes 'Cendrawasih Network base Openwrt 23.05 rc. Original compiled by Ophub and patching by manssizz'
	option timezone 'WIB-7'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	list server 'quiz.int.vidio.com'
	list server 'sb.scorecardresearch.com'
	list server 'quiz.staging.vidio.com'